Password(){
    case $1 in 
        mysql)
            sum="`echo $SSH_HOST | sha256sum | awk '{print $1}'`"
            
            echo `grep "$sum" $CONFIG_DIR/auth/mysql  | awk '{print $2}'`
            ;;
        docker)
            sum="`echo $DOCKER_REGISTRY | sha256sum   | awk '{print $1}'`"
            echo `grep "$sum" $CONFIG_DIR/auth/docker | awk '{print $2}'`
            ;;
    esac
}
